    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "format": "prettier --write \"src/**/*.+(js|ts|json)\"",
    "compile": "tsc"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
